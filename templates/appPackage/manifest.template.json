{
    "$schema": "https://developer.microsoft.com/json-schemas/teams/v1.11/MicrosoftTeams.schema.json",
    "manifestVersion": "1.11",
    "version": "1.0.0",
    "localizationInfo": {
        "defaultLanguageTag": "en-us"
    },
    "id": "{{state.fx-resource-appstudio.teamsAppId}}",
    "packageName": "com.microsoft.teams.extension",
    "developer": {
        "name": "AndorHealth",
        "websiteUrl": "https://andor.app/",
        "privacyUrl": "https://andor.app/privacy",
        "termsOfUseUrl": "https://andor.app/termsofuse"
    },
    "icons": {
        "color": "resources/logo.png",
        "outline": "resources/logo.png"
    },
    "name": {
        "short": "AndorTyto",
        "full": "AndorTyto App"
    },
    "description": {
        "short": "AndorTyto app to pull Tyto care video feeds",
        "full": "AndorTyto app to pull Tyto care video feeds"
    },
    "accentColor": "#FFFFFF",
    "bots": [],
    "composeExtensions": [],
    "configurableTabs": [
        {
            "configurationUrl": "{{{state.fx-resource-frontend-hosting.endpoint}}}{{{state.fx-resource-frontend-hosting.indexPath}}}/config",
            "canUpdateConfiguration": true,
            "scopes": [
                "team",
                "groupchat"
            ],
            "context": [
                "privateChatTab",
                "meetingSidePanel",
                "meetingStage",
                "meetingChatTab"
            ]
        }
    ], 
    "staticTabs": [
        {
            "entityId": "index",
            "name": "Personal Tab",
            "contentUrl": "{{{state.fx-resource-frontend-hosting.endpoint}}}{{{state.fx-resource-frontend-hosting.indexPath}}}/tab",
            "websiteUrl": "{{{state.fx-resource-frontend-hosting.endpoint}}}{{{state.fx-resource-frontend-hosting.indexPath}}}/tab",
            "scopes": [
                "personal"
            ]
        }
    ],
    "permissions": [
        "identity",
        "messageTeamMembers"
    ],
    "validDomains": []
}